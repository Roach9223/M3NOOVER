    "clean": "pnpm -r clean",
    "fetch-stripe-prices": "tsx scripts/fetch-stripe-prices.ts"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "tsx": "^4.19.0"
  },
  "keywords": [
    "fitness",
    "coaching",
    "athletic",
    "training"
  ],
  "author": "",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.17.0"
  }

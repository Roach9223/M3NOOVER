{
  "name": "m3noover",
  "version": "0.1.0",
  "private": true,
  "description": "M3NOOVER - Building Strong Bodies, Disciplined Minds, and Confident Athletes",
  "scripts": {
    "dev:web": "pnpm --filter @m3noover/web dev",
    "dev:app": "pnpm --filter @m3noover/app dev",
    "build:web": "pnpm --filter @m3noover/web build",
    "build:app": "pnpm --filter @m3noover/app build",
    "build": "pnpm -r build",
